<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MnemonicsContainerView">
        <attr name="isCounterEnabled" format="boolean"/>
        <attr name="isDraggable" format="boolean"/>
        <attr name="itemBackground" format="reference"/>
        <attr name="itemEmptyBackground" format="reference"/>
        <attr name="itemCounterTextColor" format="color"/>
        <attr name="itemTextColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="SwipeDisablingViewPager">
        <attr name="isSwipeEnabled" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ExpandableCardView">
        <attr name="titleText" format="reference"/>
        <attr name="contentText" format="reference"/>
        <attr name="icon" format="reference"/>
        <attr name="inner_view" format="reference"/>
        <attr name="expandOnClick" format="boolean"/>
        <attr name="animationDuration" format="integer"/>
        <attr name="startExpanded" format="boolean"/>
    </declare-styleable>
</resources>